<Window x:Class="tic_tac_toe.Splashscreenos"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:tic_tac_toe"
        mc:Ignorable="d"
        Title="Splashscreenos" Height="450" Width="800" WindowStartupLocation="CenterScreen" ContentRendered="Window_ContentRendered" WindowStyle="None" >
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="Pictures/gamessplashscreenos.jpg"/>
        </Grid.Background>
        <ProgressBar HorizontalAlignment="Center" Height="12" Margin="0,0,0,100" VerticalAlignment="Bottom" Width="400" x:Name="progressbar" IsIndeterminate="True" Foreground="#FF09B4FF" Background="#FF0500BF" BorderBrush="#FF0500BF"/>
        
    </Grid>
</Window>
